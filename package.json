{
  "name": "flaka",
  "version": "1.0.0",
  "description": "Streaming player for web player",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build": "tsc",
    "docs": "typedoc --out docs src/index.ts",
    "install": "npm run build",
    "lint": "eslint 'src' --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:staged": "jest --bail --findRelatedTests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rhapsody/flaka.git"
  },
  "author": "Antonio Novoselnik <antonio.novoselnik@napster.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Rhapsody/flaka/issues"
  },
  "homepage": "https://github.com/Rhapsody/flaka#readme",
  "dependencies": {
    "shaka-player": "^3.0.10",
    "typedoc": "^0.20.35"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint src/**/*.ts --fix",
      "bash -c tsc --noEmit",
      "jest --bail --findRelatedTests"
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
